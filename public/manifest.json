{
  "manifest_version": 3,
  "name": "Unscramm",
  "version": "0.0.1",
  "description": "Visualize text transformations and spelling corrections",
  "action": {
    "default_popup": "index.html",
    "default_title": "Unscramm"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ]
}
